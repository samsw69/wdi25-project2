<div class="container">

<h1>Login</h1>
<div id=Logo>
    <img class="img-fluid" src="../assets/images/chickenRun.png" alt="Chicken Run">
</div>

<form method="POST" action="/login">
  <div>
    <br><br>
    <label for="email">Email</label>
    <input type="text" name="email" id="email" required>
  </div>
  <div>
    <label for="password">Password</label>
    <input type="password" name="password" id="password" required>
  </div>

  <div>
    <button type="button" class="btn btn-outline-info waves-effect">Login</button>
  </div>
</form>


<a href="<%= oauth.facebook.getLoginURL() %>">Log in with Facebook</a>
</div>
